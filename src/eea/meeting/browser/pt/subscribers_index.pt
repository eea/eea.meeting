<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"
  metal:use-macro="here/main_template/macros/master"
  i18n:domain="eea">
  <body>
    <div metal:fill-slot="content-core" >
       <p>Users subscribed to this meeting</p>
       <table class="listing">
        <thead>
          <tr>
            <th>First name</th>
            <th>Last name</th>
            <th>UID</th>
            <th>State</th>
          </tr>
        <thead>
        <tbody>
          <tr tal:repeat="subscriber python:context.get_subscribers()">
            <td tal:content="python:subscriber.firstname" />
            <td tal:content="python:subscriber.lastname" />
            <td>
              <a tal:attributes="href subscriber/absolute_url" tal:content="subscriber/uid" />
            </td>
            <td>
              <span tal:content="subscriber/state" />
            </td>
          </tr>
        </tbody>
        <tfoot>
        </tfoot>
      </table>
    </div>
  </body>
</html>
