<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"
  metal:use-macro="here/main_template/macros/master"
  i18n:domain="eea">
  <body>
    <div metal:fill-slot="content-core" >
      <p tal:condition="python: context.is_registered()">
        You are registered for this meeting.
      </p>
      <tal:block condition="context/can_register">
        <p tal:condition="context/is_anonymous">
          <a tal:attributes="href string:${context/absolute_url}/subscribers/++add++eea.meeting.subscriber">Sign up</a>
          to participate in this meeting
        </p>
      </tal:block>
      <form name="folderContentsForm"
        method="post"
        action="folder_object"
        tal:attributes="action context/absolute_url"
        class="kssattr-serveraction-foldercontents_update_table">
        <div tal:replace="structure view/contents_table"></div>
        <input tal:replace="structure context/@@authenticator/authenticator" />
      </form>
    </div>
  </body>
</html>
